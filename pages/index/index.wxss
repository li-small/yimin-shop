.index_swiper swiper {
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_nav {
  display: flex;
}
.index_nav navigator {
  flex: 1;
  padding: 20rpx;
}
.index_nav navigator image {
  width: 100%;
}
.index_floor .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_group {
  overflow: hidden;
}
.index_floor .floor_group .floor_goods navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .floor_goods navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_goods navigator:nth-child(2),
.index_floor .floor_group .floor_goods navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_goods navigator image {
  width: 100%;
  height: 100%;
}
